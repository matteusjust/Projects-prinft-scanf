/*Calculates the volume of a 10 meter radius sphere*/ 

#include <stdio.h>

#define PI 3.14f 
#define RADIUS 10.0f
#define FRACTION (4.0f / 3.0f) 

int main(void)
{
    float volume;
    
    volume = FRACTION * PI * RADIUS * RADIUS * RADIUS;
    printf ("Volume of sphere (meters) : %.2f\n", volume);
    
    return 0;
}
