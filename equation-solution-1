/* Give a value to x and resolve an equation*/ 

#include <stdio.h>

int main(void)
{
    int x, fr; 
    
    printf ("Enter a value for x ");
    scanf ("%d", &x);
    
    fr = 3*(x*x*x*x*x) + 2*(x*x*x*x) - 5*(x*x*x) - (x*x) + 7*(x) - 6;
    printf ("The solution is: %d\n", fr);
    
    return 0;

}

/* The equation is 3x^5 + 2x^4 - 5x^3 - x^2 + 7x -6 */
